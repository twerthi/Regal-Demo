{
    "Resources": {
        "regalVPC" : {
            "Type" : "AWS::EC2::VPC",
            "Properties" : {
                "CidrBlock" : "10.20.0.0/16",
                "EnableDnsSupport" : "true",
                "EnableDnsHostnames" : "true",
                "Tags" : [ 
                    {"Key" : "Name", "Value" : "shawn-sesna-vpc"} 
                ]
            }
        },
        "regalIGW" : {
            "Type" : "AWS::EC2::InternetGateway",
            "Properties" : {
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-igw"}
                ]
            }
        },
        "regalIGWVPC" : {
            "Type" : "AWS::EC2::VPCGatewayAttachment",
            "Properties" : {
                "InternetGatewayId" : { "Ref" : "regalIGW"},
                "VpcId" : { "Ref" : "regalVPC"}
            },
            "DependsOn" : [
                "regalIGW",
                "regalVPC"
            ]
        },
        "regalRT" : {
            "Type" : "AWS::EC2::RouteTable",
            "Properties" : {
                "VpcId" : { "Ref" : "regalVPC"},
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-RT"}
                ]
            },
            "DependsOn" : "regalVPC"            
        },
        "regalSubnet1" : {
            "Type" : "AWS::EC2::Subnet",
            "Properties" : {
                "VpcId" : {"Ref" : "regalVPC"},
                "CidrBlock" : "10.20.1.0/24",
                "AvailabilityZone" : "",
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-subnet1"}
                ],
                "MapPublicIpOnLaunch" : true
            },
            "DependsOn" : "regalVPC"
        },
        "regalSubnet2" : {
            "Type" : "AWS::EC2::Subnet",
            "Properties" : {
                "VpcId" : {"Ref" : "regalVPC"},
                "CidrBlock" : "10.20.2.0/24",
                "AvailabilityZone" : "",
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-subnet2"}
                ],
                "MapPublicIpOnLaunch" : true
            },
            "DependsOn" : "regalVPC"
        },
        "regalRTA1" : {
            "Type" : "AWS::EC2::SubnetRouteTableAssociation",
            "Properties" : {
                "RouteTableId" : {"Ref" : "regalRT"},
                "SubnetId" : {"Ref" : "regalSubnet1"}
            },
            "DependsOn" : [
                "regalSubnet1",
                "regalSubnet2"
            ]
        },
        "regalRTA2" : {
            "Type" : "AWS::EC2::SubnetRouteTableAssociation",
            "Properties" : {
                "RouteTableId" : {"Ref" : "regalRT"},
                "SubnetId" : {"Ref" : "regalSubnet2"}
            },
            "DependsOn" : [
                "regalSubnet1",
                "regalSubnet2"
            ]
        },        
        "regalSG" : {
            "Type" : "AWS::EC2::SecurityGroup",
            "Properties" : {
                "GroupName" : "shawn-sesna-security-group",
                "GroupDescription" : "Regal demo security group",
                "SecurityGroupEgress" : [
                    {
                        "IPProtocol" : "tcp",
                        "FromPort" : 0,
                        "ToPort" : 0,
                        "CidrIp" : "0.0.0.0/0"
                    }
                ],
                "SecurityGroupIngress" : [
                    {
                        "IPProtocol" : "tcp",
                        "FromPort" : 5432,
                        "ToPort" : 5432,
                        "CidrIp" : "0.0.0.0/0"
                    }
                ],
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-securitygroup"}
                ],
                "VpcId" : { "Ref" : "regalVPC"}
            }
        },
        "regalDBSubnet" : {
            "Type" : "AWS::RDS::DBSubnetGroup",
            "Properties" : {
                "DBSubnetGroupName" : "shawn-sesna-db-subnetgroup",
                "DBSubnetGroupDescription" : "Regal demo Aurora postres",
                "SubnetIds" : [
                    {"Ref" : "regalSubnet1"},
                    {"Ref" : "regalSubnet2"}
                ]
            },
            "DependsOn" : [
                "regalSubnet1",
                "regalSubnet2"
            ]
        },
        "regalPostgresCluster" : {
            "Type" : "AWS::RDS::DBCluster",
            "Properties" : {
                "VpcSecurityGroupIds" : [
                    {"Ref" : "regalSG"}
                ],
                "Engine" : "aurora-postgresql",
                "EngineMode" : "provisioned",
                "EngineVersion" : "15.4",
                "MasterUsername" : "ReplaceMe!",
                "MasterUserPassword" : "ReplaceMe!",
                "DBClusterIdentifier" : "ReplaceMe!",
                "DBSubnetGroupName" : {"Ref" : "regalDBSubnet"},
                "BackupRetentionPeriod" : 7,
                "DeletionProtection" : false,
                "Port" : 5432
            }
        },
        "regalPostgresInstance1" : {
            "Type": "AWS::RDS::DBInstance",
            "Properties" : {
                "Engine" : "aurora-postgresql",
                "DBClusterIdentifier" : {"Ref" : "regalPostgresCluster"},
                "DBSubnetGroupName" : {"Ref" : "regalDBSubnet"},
                "PubliclyAccessible" : true,
                "DBInstanceClass" : "db.t3.medium"
            }
        }
    }
 }