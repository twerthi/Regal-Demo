{
    "Resources": {
        "regalVPC" : {
            "Type" : "AWS::EC2::VPC",
            "Properties" : {
                "CidrBlock" : "10.0.0.0/16",
                "EnableDnsSupport" : "true",
                "EnableDnsHostnames" : "true",
                "Tags" : [ 
                    {"Key" : "Name", "Value" : "shawn-sesna-vpc"} 
                ]
            }
        },
        "regalIGW" : {
            "Type" : "AWS::EC2::InternetGateway",
            "Properties" : {
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-igw"}
                ]
            }
        },
        "regalIGWVPC" : {
            "Type" : "AWS::EC2::VPCGatewayAttachment",
            "Properties" : {
                "InternetGatewayId" : { "Ref" : "regalIGW"},
                "VpcId" : { "Ref" : "regalVPC"}
            },
            "DependsOn" : [
                "regalIGW",
                "regalVPC"
            ]
        },
        "regalRT" : {
            "Type" : "AWS::EC2::RouteTable",
            "Properties" : {
                "VpcId" : { "Ref" : "regalVPC"},
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-RT"}
                ]
            },
            "DependsOn" : "regalVPC"            
        },
        "regalSubnet" : {
            "Type" : "AWS::EC2::Subnet",
            "Properties" : {
                "VpcId" : {"Ref" : "regalVPC"},
                "CidrBlock" : "10.0.0.0/24",
                "AvailabilityZone" : ["us-east-1a", "us-east-1b"],
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-subnet"}
                ]
            }
        },
        "regalRTA" : {
            "Type" : "AWS::EC2::SubnetRouteTableAssociation",
            "Properties" : {
                "RouteTableId" : {"Ref" : "regalRT"}
            },
            "DependsOn" : "regalSubnet"
        },
        "regalSG" : {
            "Type" : "AWS::EC2::SecurityGroup",
            "Properties" : {
                "GroupName" : "shawn-sesna-security-group",
                "GroupDescription" : "Regal demo security group",
                "SecurityGroupEgress" : [
                    {
                        "IPProtocol" : "tcp",
                        "FromPort" : 0,
                        "ToPort" : 0,
                        "CidrIp" : "0.0.0.0/0"
                    }
                ],
                "SecurityGroupIngrss" : [
                    {
                        "IPProtocol" : "tcp",
                        "FromPort" : 5432,
                        "ToPort" : 5432,
                        "CidrIp" : "0.0.0.0/0"
                    }
                ],
                "Tags" : [
                    {"Key" : "Name", "Value" : "shawn-sesna-securitygroup"}
                ],
                "VpcId" : { "Ref" : "regalVPC"}
            }
        },
        "regalDBSubnet" : {
            "Type" : "AWS::RDS::DBSubnetGroup",
            "Properties" : {
                "DBSubnetGroupName" : "shawn-sesna-db-subnetgroup",
                "DBSubnetGroupDescription" : "Regal demo Aurora postres",
                "SubnetIds" : [
                    {"Ref" : "regalSubnet"}
                ]
            },
            "DependsOn" : "regalSubnet"
        }
    }
 }